    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.19",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "jest-html-reporter": "^3.7.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
